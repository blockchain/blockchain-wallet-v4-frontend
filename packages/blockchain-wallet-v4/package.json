{
  "name": "blockchain-wallet-v4",
  "version": "1.0.1",
  "description": "Functional library for handling Blockchain.info wallets.",
  "license": "ISC",
  "author": {
    "name": "Blockchain.info",
    "url": "https://blockchain.info"
  },
  "main": "lib/index.js",
  "module": "src/index.js",
  "scripts": {
    "compile": "babel --presets es2015,stage-2 -d lib/ --ignore spec.js src/ --copy-files",
    "coverage": "cross-env ./../../node_modules/.bin/jest --coverage",
    "start": "babel-node src/index.js",
    "test": "cross-env ./../../node_modules/.bin/jest",
    "test:build": "npm run compile",
    "test:debug": "cross-env node --inspect-brk ./../../node_modules/.bin/jest --runInBand",
    "test:watch": "cross-env ./../../node_modules/.bin/jest --watchAll"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/rootSaga.js"
    ],
    "coverageDirectory": "<rootDir>/../../coverage/blockchain-wallet-v4",
    "coverageReporters": [
      "json",
      "html"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/lib"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/lib"
    ]
  },
  "standard": {
    "global": [
      "describe",
      "it",
      "expect"
    ]
  },
  "dependencies": {
    "bech32": "1.1.3",
    "big-rational": "0.10.7",
    "bigi": "1.4.2",
    "bignumber.js": "5.0.0",
    "bip39": "2.3.1",
    "bitcoin-coinify-client": "0.7.6",
    "bitcoin-sfox-client": "0.4.0",
    "bitcoinforksjs-lib": "git://github.com/blockchain/bitcoinjs-lib.git#opt-in-bitcoincash-sighash",
    "bitcoinjs-lib": "3.1.1",
    "bitcoinjs-message": "2.0.0",
    "bs58": "4.0.1",
    "cashaddress": "1.1.0",
    "chai-immutable": "1.6.0",
    "daggy": "1.2.0",
    "data.either": "1.5.1",
    "data.maybe": "1.2.2",
    "data.task": "3.1.1",
    "es6-promise": "4.1.0",
    "ethereumjs-tx": "1.3.3",
    "ethereumjs-util": "5.1.2",
    "ethereumjs-wallet": "0.6.0",
    "extendable-immutable": "1.3.3",
    "fast-memoize": "2.2.8",
    "fisher-yates": "1.0.3",
    "futurize": "1.2.0",
    "immutable": "3.8.1",
    "immutable-ext": "1.1.2",
    "isomorphic-fetch": "2.2.1",
    "pbkdf2": "3.0.12",
    "query-string": "5.0.1",
    "ramda": "0.25.0",
    "ramda-lens": "git+https://github.com/ramda/ramda-lens.git",
    "read-blob": "1.1.0",
    "redux": "3.6.0",
    "redux-immutable": "3.1.0",
    "redux-saga": "0.16.0",
    "scryptsy": "2.0.0",
    "seedrandom": "2.4.3",
    "unorm": "1.4.1"
  },
  "watch": {
    "compile": "src/**/*.js"
  }
}
