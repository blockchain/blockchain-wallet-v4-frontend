  },
  "devDependencies": {
    "jest-styled-components": "6.3.3",
    "@types/applepayjs": "3.0.4",
    "@types/enzyme": "3.10.11",
    "@testing-library/react-hooks": "6.0.0",
    "@graphql-codegen/cli": "2.5.0",
    "@graphql-codegen/introspection": "2.1.1",
