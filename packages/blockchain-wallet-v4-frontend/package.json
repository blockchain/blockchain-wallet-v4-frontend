{
  "name": "blockchain-wallet-v4-frontend",
  "version": "0.1.0",
  "description": "Frontend wallet application.",
  "license": "AGPL-3.0-or-later",
  "author": {
    "name": "Blockchain.com",
    "url": "https://blockchain.com"
  },
  "main": "index.js",
  "scripts": {
    "analyze": "cross-env-shell ANALYZE=true NODE_ENV=production webpack-cli --config webpack.config.ci.js --progress --color",
    "build:dev": "cross-env-shell APP_ENV_LOCAL=development NODE_ENV=development webpack-cli --config webpack.config.dev.js --progress --color",
    "build:staging": "cross-env-shell APP_ENV_LOCAL=staging NODE_ENV=development webpack-cli --config webpack.config.dev.js --progress --color",
    "build:prod": "cross-env-shell APP_ENV_LOCAL=production NODE_ENV=development webpack-cli --config webpack.config.dev.js --progress --color",
    "ci:test:frontend": "yarn test --runInBand",
    "ci:compile": "cross-env-shell NODE_OPTIONS=\"--max-old-space-size=8192\" NODE_ENV=production webpack-cli --config webpack.config.ci.js",
    "clean": "cross-env rimraf node_modules && rimraf build",
    "coverage": "cross-env npx jest --coverage --silent",
    "debug:prod": "cross-env-shell NODE_ENV=production webpack-dev-server --config webpack.debug.js --progress --color",
    "link:resolved:paths": "ln -sf $(pwd)/src/** ./node_modules && ln -sf $(pwd)/../../packages/** ./node_modules",
    "manage:translations": "formatjs extract './src/!(assets)/**/*.{ts,tsx,js,jsx}' --out-file './src/assets/locales/en.json' --id-interpolation-pattern '[sha512:contenthash:base64:6]' --format simple",
    "start:dev": "cross-env-shell APP_ENV_LOCAL=development NODE_ENV=development webpack-dev-server --config webpack.config.dev.js --progress --color",
    "start:staging": "cross-env-shell APP_ENV_LOCAL=staging NODE_ENV=development webpack-dev-server --config webpack.config.dev.js --progress --color",
    "start:prod": "cross-env-shell DISABLE_SSL=true APP_ENV_LOCAL=production NODE_ENV=development webpack-dev-server --config webpack.config.dev.js --progress --color",
    "storybook:build": "build-storybook",
    "storybook:serve": "start-storybook -p 6006",
    "storybook:deploy": "yarn deploy-storybook --existing-output-dir=./storybook-static",
    "test": "cross-env npx jest --runInBand",
    "test:build": "echo 'No precomplilation required for tests to execute.'",
    "test:debug": "cross-env npx --inspect-brk jest --runInBand",
    "test:update": "cross-env npx jest -u",
    "test:watch": "cross-env npx jest --watchAll --runInBand",
    "codegen": "graphql-codegen --config codegen.yml",
    "graphql-codegen": "graphql-codegen --config codegen.yml"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/scenes/Exchange/ExchangeForm/**/*.{js,ts}",
      "src/store/*.{js,ts}",
      "src/utils/*.{js,ts}",
      "src/services/ValidationHelper/*.{js,ts}"
    ],
    "coverageDirectory": "<rootDir>/../../coverage/blockchain-wallet-v4-frontend",
    "coverageReporters": [
      "json",
      "html"
    ],
    "setupFiles": [
      "<rootDir>/../../config/jest/jest.shim.js",
      "<rootDir>/../../config/jest/jest.config.js"
    ],
    "snapshotSerializers": [
      "<rootDir>/../../node_modules/enzyme-to-json/serializer"
    ],
    "testPathIgnorePatterns": [
      "extracted-ts/packages"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest",
      "^.+\\.tsx?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(blockchain-info-components|@blockchain-com/components|blockchain-wallet-v4)/)"
    ],
    "modulePaths": [
      "src"
    ],
    "moduleNameMapper": {
      "\\.(pdf|jpg|jpeg|png|gif|eot|otf|svg|ttf|woff|woff2)$": "<rootDir>/src/__mocks__/fileMock.js",
      "\\.(css|scss)$": "<rootDir>/../../node_modules/identity-obj-proxy",
      "@core(.*)": "<rootDir>/../blockchain-wallet-v4/src/$1"
    },
    "testEnvironment": "jsdom"
  },
  "dependencies": {
    "@babel/polyfill": "7.12.1",
    "@blockchain-com/components": "6.1.1",
    "@blockchain-com/constellation": "0.2.8",
    "@blockchain-com/icons": "0.0.6",
    "@formatjs/intl-relativetimeformat": "7.2.10",
    "@graphql-codegen/cli": "2.5.0",
    "@reduxjs/toolkit": "1.6.0",
    "@storybook/addon-actions": "6.3.6",
    "@storybook/react": "6.3.6",
    "@veriff/incontext-sdk": "0.3.0",
    "@visx/event": "1.7.0",
    "@visx/gradient": "1.7.0",
    "@visx/scale": "1.7.0",
