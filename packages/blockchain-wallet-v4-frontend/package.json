    "bitcoinjs-lib": "5.2.0",
    "boring-avatars": "^1.7.0",
    "bowser": "2.11.0",
    "browser-locale": "1.0.3",
    "connected-react-router": "6.9.1",
    "country-region-data": "1.7.0",
    "dompurify": "2.3.6",
