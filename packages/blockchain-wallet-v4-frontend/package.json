{
  "name": "blockchain-wallet-v4-frontend",
  "version": "0.1.0",
  "description": "Frontend wallet application.",
  "license": "ISC",
  "author": {
    "name": "Blockchain.info",
    "url": "https://blockchain.info"
  },
  "main": "index.js",
  "scripts": {
    "analyze": "cross-env-shell ANALYZE=true NODE_ENV=production CI_BUILD=true webpack-cli --config webpack.config.js",
    "build:dev": "cross-env-shell NODE_ENV=development webpack-cli --config webpack.config.js --progress --colors",
    "build:prod": "cross-env-shell NODE_ENV=production webpack-cli --config webpack.config.js --progress --colors",
    "build:staging": "cross-env-shell NODE_ENV=staging webpack-cli --config webpack.config.js --progress --colors",
    "build:testnet": "cross-env-shell NODE_ENV=testnet webpack-cli --config webpack.config.js --progress --colors",
    "ci:coverage:frontend": "yarn coverage --runInBand",
    "ci:test:frontend": "yarn test --runInBand",
    "ci:compile": "cross-env-shell NODE_ENV=production CI_BUILD=true webpack-cli --config webpack.config.js --display-error-details",
    "coverage": "cross-env ./../../node_modules/.bin/jest --coverage",
    "link:resolved:paths": "ln -sf $(pwd)/src/** ./node_modules && ln -sf $(pwd)/../../packages/** ./node_modules",
    "start:dev": "cross-env-shell NODE_ENV=development webpack-dev-server --config webpack.config.js --progress --colors --watch --devtool cheap-module-source-map",
    "start:prod": "cross-env-shell NODE_ENV=production webpack-dev-server --config webpack.config.js --progress --colors --watch --devtool cheap-module-source-map",
    "start:staging": "cross-env-shell NODE_ENV=staging webpack-dev-server --config webpack.config.js --progress --colors --watch --devtool cheap-module-source-map",
    "start:testnet": "cross-env-shell NODE_ENV=testnet webpack-dev-server --config webpack.config.js --progress --colors --watch --devtool cheap-module-source-map",
    "test": "cross-env ./../../node_modules/.bin/jest",
    "test:build": "echo 'No precomplilation required for tests to execute.'",
    "test:debug": "cross-env node --inspect-brk ./../../node_modules/.bin/jest --runInBand",
    "test:watch": "cross-env ./../../node_modules/.bin/jest --watchAll",
    "manage:translations": "babel-node ./translationRunner.js"
  },
  "jest": {
    "automock": false,
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/rootSaga.js",
      "!src/data/*/actions.js",
      "!src/data/*/actionTypes.js",
      "!src/data/*/sagaRegister.js",
      "!src/data/actions.js",
      "!src/data/actionTypes.js",
      "!src/data/rootReducer.js",
      "!src/data/sagas.js",
      "!src/data/sagaRegister.js",
      "!src/data/selectors.js"
    ],
    "coverageDirectory": "<rootDir>/../../coverage/blockchain-wallet-v4-frontend",
    "coverageReporters": [
      "json",
      "html"
    ],
    "setupFiles": [
      "<rootDir>/../../config/jest/jest.shim.js",
      "<rootDir>/../../config/jest/jest.config.js"
    ],
    "snapshotSerializers": [
      "<rootDir>/../../node_modules/enzyme-to-json/serializer"
    ],
    "transform": {
      "^.+\\.jsx$": "<rootDir>/../../node_modules/babel-jest",
      "^.+\\.js$": "<rootDir>/../../node_modules/babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(blockchain-info-components|blockchain-wallet-v4)/)"
    ],
    "moduleNameMapper": {
      "\\.(pdf|jpg|jpeg|png|gif|eot|otf|svg|ttf|woff|woff2)$": "<rootDir>/src/__mocks__/fileMock.js",
      "\\.(css|scss)$": "<rootDir>/../../node_modules/identity-obj-proxy"
    }
  },
  "dependencies": {
    "awesome-phonenumber": "2.2.5",
    "base-64": "0.1.0",
    "bignumber.js": "5.0.0",
    "bip21": "2.0.2",
    "bip39": "2.3.1",
    "bitcoinforksjs-lib": "git://github.com/blockchain/bitcoinjs-lib.git#opt-in-bitcoincash-sighash",
    "bitcoinjs-lib": "3.1.1",
    "blockchain-info-components": "0.0.31",
    "blockchain-wallet-v4": "1.0.1",
    "bowser": "1.9.3",
    "browser-locale": "1.0.3",
    "connected-react-router": "4.3.0",
    "country-region-data": "^1.4.4",
    "data.either": "1.5.1",
    "data.maybe": "1.2.2",
    "es6-promise": "4.1.0",
    "fast-memoize": "2.4.0",
    "ibantools": "2.0.0",
    "libphonenumber-js": "0.4.40",
    "lodash.debounce": "4.0.8",
    "matchmedia-polyfill": "0.3.1",
    "moment": "2.22.2",
    "postal-codes-js": "^1.0.0",
    "prop-types": "15.6.1",
    "qrcode.react": "0.8.0",
    "ramda": "0.25.0",
    "react": "16.4.0",
    "react-animations": "1.0.0",
    "react-copy-to-clipboard": "5.0.1",
    "react-csv": "1.0.14",
    "react-dom": "16.4.0",
    "react-dropzone": "4.2.11",
    "react-highcharts": "16.0.2",
    "react-intl": "2.4.0",
    "react-intl-tel-input": "5.0.5",
    "react-onclickoutside": "6.7.1",
    "react-qr-reader": "2.1.0",
    "react-redux": "5.0.7",
    "react-router-bootstrap": "0.24.4",
    "react-router-dom": "4.1.1",
    "react-tooltip": "3.8.0",
    "react-transition-group": "2.2.1",
    "react-webcam": "0.5.0",
    "read-blob": "1.1.0",
    "redux": "3.6.0",
    "redux-form": "7.3.0",
    "redux-logger": "3.0.6",
    "redux-persist": "4.8.3",
    "redux-saga": "0.16.0",
    "redux-ui": "0.1.1",
    "reselect": "3.0.1",
    "sanitize-html": "1.18.2",
    "styled-components": "3.3.2",
    "zxcvbn": "4.4.2"
  }
}
