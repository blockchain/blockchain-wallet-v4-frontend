{
  "name": "blockchain-wallet-v4-frontend",
  "version": "0.0.1",
  "description": "Wallet user interface",
  "main": "index.js",
  "scripts": {
    "build": "lerna bootstrap && webpack --config webpack.config.js --progress --colors",
    "build:prod": "lerna bootstrap && webpack -p --config webpack.config.js --progress --colors",
    "start": "webpack-dev-server --config webpack.config.js --progress --colors --watch",
    "start:prod": "webpack-dev-server -p --config webpack.config.js --progress --colors --watch",
    "generate": "babel-node ./tools/generateDefaultLocale.js",
    "test": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blockchain/blockchain-wallet-v4-frontend.git"
  },
  "keywords": [
    "react",
    "wallet",
    "blockchain"
  ],
  "author": {
    "name": "(Dream Team) Blockchain.info",
    "url": "https://blockchain.info"
  },
  "contributors": [
    "Justin Tormey",
    "Jaume Pernas",
    "Guillaume Marquilly"
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/blockchain/blockchain-wallet-v4-frontend/issues"
  },
  "homepage": "https://github.com/blockchain/blockchain-wallet-v4-frontend#readme",
  "jest": {
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|svg|ttf|woff|woff2)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss)$": "identity-obj-proxy",
      "^locales(.*)$": "<rootDir>/src/assets/locales/$1"
    }
  },
  "standard": {
    "globals": [
      "jest",
      "test",
      "describe",
      "context",
      "before",
      "beforeEach",
      "after",
      "afterEach",
      "it",
      "expect"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^8.0.2",
    "babel-jest": "^21.2.0",
