{
  "name": "blockchain-wallet-v4-frontend",
  "version": "0.1.0",
  "description": "Frontend wallet application.",
  "license": "ISC",
  "author": {
    "name": "Blockchain.info",
    "url": "https://blockchain.info"
  },
  "main": "index.js",
  "scripts": {
    "analyze": "ANALYZE=true NODE_ENV=production webpack-cli --config webpack.config.js",
    "build:dev": "NODE_ENV=development webpack-cli --config webpack.config.js --progress --colors",
    "build:prod": "NODE_ENV=production webpack-cli --config webpack.config.js --progress --colors",
    "build:staging": "NODE_ENV=staging webpack-cli --config webpack.config.js --progress --colors",
    "build:testnet": "NODE_ENV=testnet webpack-cli --config webpack.config.js --progress --colors",
    "ci:compile": "NODE_ENV=production CI_BUILD=true webpack-cli --config webpack.config.js --display-error-details",
    "coverage": "./../../node_modules/.bin/jest --coverage",
    "generate": "babel-node ./tools/generateDefaultLocale.js",
    "run:prod": "npm run ci:compile && LOCAL_PROD=true node ./../../server.js",
    "start:dev": "NODE_ENV=development webpack-dev-server --config webpack.config.js --progress --colors --watch",
    "start:prod": "NODE_ENV=production webpack-dev-server --config webpack.config.js --progress --colors --watch",
    "start:staging": "NODE_ENV=staging webpack-dev-server --config webpack.config.js --progress --colors --watch",
    "start:testnet": "NODE_ENV=testnet webpack-dev-server --config webpack.config.js --progress --colors --watch",
    "test": "./../../node_modules/.bin/jest",
    "test:build": "echo 'No precomplilation required for tests to execute.'",
    "test:debug": "node --inspect-brk ./../../node_modules/.bin/jest --runInBand",
    "test:watch": "./../../node_modules/.bin/jest --watchAll"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/actions.js",
      "!src/**/actionTypes.js"
    ],
    "coverageDirectory": "<rootDir>/../../coverage/blockchain-wallet-v4-frontend",
    "coverageReporters": [
      "json",
      "html"
    ],
    "setupFiles": [
      "<rootDir>/../../config/jest/jest.shim.js",
      "<rootDir>/../../config/jest/jest.config.js"
    ],
    "snapshotSerializers": [
      "<rootDir>/../../node_modules/enzyme-to-json/serializer"
    ],
    "transform": {
      "^.+\\.jsx$": "<rootDir>/../../node_modules/babel-jest",
      "^.+\\.js$": "<rootDir>/../../node_modules/babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(blockchain-info-components|blockchain-wallet-v4)/)"
    ]
  },
  "dependencies": {
    "base-64": "^0.1.0",
    "bignumber.js": "^5.0.0",
    "bip21": "^2.0.1",
    "bip39": "^2.3.1",
    "bitcoin-coinify-client": "^0.7.6",
    "bitcoin-sfox-client": "^0.2.6",
    "bitcoinforksjs-lib": "git://github.com/blockchain/bitcoinjs-lib.git#opt-in-bitcoincash-sighash",
    "bitcoinjs-lib": "^3.1.1",
    "blockchain-info-components": "^0.0.1",
    "blockchain-wallet-v4": "^1.0.1",
    "connected-react-router": "^4.2.3",
    "data.either": "~1.5.1",
    "data.maybe": "^1.2.2",
    "es6-promise": "^4.1.0",
    "fast-memoize": "^2.2.8",
    "libphonenumber-js": "^0.4.40",
    "moment": "^2.19.2",
    "prop-types": "^15.6.1",
    "qrcode.react": "^0.7.2",
    "ramda": "^0.25.0",
    "react": "^16.3.0",
    "react-animations": "^1.0.0",
    "react-browser-detection": "^1.0.4",
    "react-copy-to-clipboard": "^5.0.1",
    "react-csv": "^1.0.14",
    "react-dom": "^16.3.0",
    "react-dropzone": "^4.2.8",
    "react-highcharts": "^13.0.0",
    "react-intl": "^2.3.0",
    "react-intl-tel-input": "^5.0.1",
    "react-onclickoutside": "^6.7.1",
    "react-qr-reader": "^2.0.0",
    "react-redux": "^5.0.5",
    "react-router-bootstrap": "^0.24.4",
    "react-router-dom": "^4.1.1",
    "react-transition-group": "^2.2.1",
    "read-blob": "^1.1.0",
    "redux": "^3.6.0",
    "redux-form": "^6.8.0",
    "redux-logger": "^3.0.6",
    "redux-persist": "^4.8.3",
    "redux-saga": "^0.16.0",
    "redux-ui": "^0.1.1",
    "styled-components": "^3.2.4",
    "zxcvbn": "^4.4.2"
  }
}
