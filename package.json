{
  "name": "dream-wallet-frontend",
  "version": "0.0.1",
  "description": "Wallet user interface",
  "main": "index.js",
  "scripts": {
    "build": "./node_modules/.bin/webpack --config webpack.config.dev.js --verbose",
    "start": "./node_modules/.bin/webpack-dev-server --config webpack.config.dev.js --devtool eval --progress --colors",
    "generate": "./node_modules/.bin/babel-node ./tools/generateDefaultLocale.js",
    "test": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blockchain/dream-wallet-frontend.git"
  },
  "keywords": [
    "react",
    "wallet",
    "blockchain"
  ],
  "author": {
    "name": "(Dream Team) Blockchain.info",
    "url": "https://blockchain.info"
  },
  "contributors": [
    "Justin Tormey",
    "Jaume Pernas",
    "Guillaume Marquilly"
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/blockchain/dream-wallet-frontend/issues"
  },
  "homepage": "https://github.com/blockchain/dream-wallet-frontend#readme",
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|svg|ttf|woff|woff2)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss)$": "identity-obj-proxy",
      "^locales(.*)$": "<rootDir>/src/assets/locales/$1"
    }
  },
  "standard": {
    "globals": [
      "jest",
      "test",
      "describe",
      "context",
      "before",
      "beforeEach",
      "after",
      "afterEach",
      "it",
      "expect"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-plugin-react-intl": "^2.3.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.2",
    "eslint": "^3.19.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^7.0.1",
    "eslint-watch": "^3.1.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.28.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^20.0.4",
    "jest-css-modules": "^1.1.0",
    "node-sass": "^4.5.3",
    "react-test-renderer": "^15.5.4",
    "redux-mock-store": "^1.2.3",
    "sass-loader": "^6.0.5",
    "sass-resources-loader": "^1.2.1",
    "standard": "^10.0.2",
    "style-loader": "^0.18.0",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.4.5",
    "yaml-loader": "^0.4.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "bootstrap": "4.0.0-alpha.6",
    "connected-react-router": "^4.2.1",
    "data.maybe": "^1.2.2",
    "dream-wallet": "blockchain/dream-wallet",
